@using PencilCase.Web.Services
@inject FragmentApi FragmentApi;

@if (Generators != null)
{
    foreach (var fragment in Generators)
    {
        <FragmentGeneratorCard FragmentGenerator=@fragment/>
    }
}
else
{
    <MudText Typo="Typo.body2">Error retrieving available study guide fragments. :\</MudText>
}

@code{
    [Parameter]
    public List<Models.FragmentGenerator>? Generators;
}