<MudIconButton Icon="@Icons.Material.Rounded.Autorenew" aria-label="regenerate" OnClick="OnRegenerateClick" />
<MudIconButton Icon="@Icons.Material.Rounded.Delete" aria-label="delete" OnClick="OnDeleteClick" />

@code {
    [Parameter] public String Name { get; set; } = String.Empty;
    [Parameter] public Action<String>? OnRegenerate { get; set; } 
    [Parameter] public Action<String>? OnDelete { get; set; } 

    private void OnRegenerateClick(){
        if(OnRegenerate != null)
            OnRegenerate(Name);
    }

    private void OnDeleteClick(){
        if(OnDelete != null)
            OnDelete(Name);
    }
}